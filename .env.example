# Renovation Estimator Environment Variables
# Copy this file to .env and replace with your actual values

# OpenAI API Key (Uncomment if actually using OpenAI API)
# OPENAI_API_KEY=your-api-key-here

# Optional: DeepLake for vector store (uncomment if using actual vector DB)
# DEEPLAKE_API_KEY=your-deeplake-key-here

# Feature flags
MOCK_DATA=true
ENABLE_EVALUATION=true
ENABLE_FINE_TUNING=false

# Debug settings
DEBUG=false
LOG_LEVEL=INFO 